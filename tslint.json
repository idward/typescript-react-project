{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "interface-name": false,
    "ordered-imports": false,
    "import-name": false,
    "variable-name": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ],
    "include": ["src"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
